{
  "name": "shipyard",
  "version": "3.1.1",
  "license": "MIT",
  "main": "server",
  "author": "KhulnaSoft Dev <info@khulnasoft.com> (https://khulnasoft.com)",
  "scripts": {
    "start": "node server",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --skip-plugins pwa --no-module && cp -r src/assets/fonts dist/fonts && cp -r src/assets/widget-resources dist/widget-resources",
    "build:clean": "rm -rf dist && NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --clean --skip-plugins pwa",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "postinstall": "pnpm prepare",
    "pm2-start": "npx pm2 start server.js",
    "build-watch": "vue-cli-service build --watch --mode production",
    "build-and-start": "NODE_OPTIONS=--openssl-legacy-provider pnpm-run-all --parallel build start",
    "validate-config": "node services/config-validator",
    "health-check": "node services/healthcheck",
    "dependency-audit": "pnpm audit",
    "sass-migrate": "find src -name '*.css' -exec sh -c 'sass-migrator convert \"$1\" --migrate-deps --load-path=src' _ {} \\;",
    "watch:sass": "./scripts/update-sass.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@fontsource/audiowide": "5.2.5",
    "@fontsource/cutive-mono": "5.2.5",
    "@fontsource/francois-one": "5.2.5",
    "@fontsource/podkova": "5.2.6",
    "@fontsource/roboto": "5.2.5",
    "@fontsource/shrikhand": "5.2.5",
    "@fontsource/sniglet": "5.2.5",
    "@fontsource/vt323": "5.2.5",
    "@formschema/native": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "^3.0.0",
    "@sentry/tracing": "^7.0.0",
    "@sentry/vue": "^7.0.0",
    "@vue/composition-api": "^1.7.1",
    "aws-sdk": "^2.1.0",
    "axios": "^1.6.0",
    "bcryptjs": "3.0.2",
    "bootstrap": "^5.3.0",
    "bootstrap-vue": "^2.23.1",
    "chart.js": "^4.0.0",
    "connect-history-api-fallback": "2.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-basic-auth": "1.2.1",
    "express-rate-limit": "7.5.0",
    "express-session": "^1.17.3",
    "frappe-charts": "^1.6.2",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "keycloak-js": "^22.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.0",
    "oidc-client-ts": "^2.4.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "register-service-worker": "^1.7.2",
    "rss-parser": "^3.13.0",
    "rsup-progress": "^1.0.0",
    "simple-icons": "^11.0.0",
    "v-jsoneditor": "^1.0.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.7.16",
    "vue-i18n": "^8.28.2",
    "vue-js-modal": "^2.0.0",
    "vue-json-tree-view": "^2.1.6",
    "vue-material-tabs": "^1.0.0",
    "vue-router": "^3.6.5",
    "vue-select": "^3.20.2",
    "vue-swatches": "^2.0.0",
    "vue-toasted": "^1.1.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^4.0.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.36.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "7.1.2",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-vue": "^7.20.0",
    "file-loader": "6.2.0",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "msw": "^2.7.3",
    "node-polyfill-webpack-plugin": "4.1.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "sass": "^1.32.13",
    "sass-loader": "^13.3.0",
    "sass-migrator": "^1.7.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "typescript": "^5.8.3",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "vue-cli-plugin-yaml": "^1.0.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "2.2.8",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=16.0.0 <21.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "airbnb-base"
    ],
    "ignorePatterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/@/**"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "arrow-parens": 0,
      "no-else-return": 0,
      "prefer-regex-literals": "off"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false
    }
  },
  "babel": {
    "presets": [
      [
        "@vue/app",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.{js,vue,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "pnpm@8.15.4"
}
